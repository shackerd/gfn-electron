{
  "name": "geforcenow-electron",
  "appId": "com.github.hmlendea.${name}",
  "productName": "GeForce NOW",
  "version": "2.2.0",
  "description": "A Linux desktop web app for GeForce NOW",
  "main": "scripts/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --publish never"
  },
  "build": {
    "asar": false,
    "linux": {
      "target": [
        "zip",
        "AppImage"
      ],
      "category": "Games",
      "artifactName": "${name}_${version}_linux.${ext}"
    },
    "extraFiles": [
      "com.github.hmlendea.${name}.desktop"
    ]
  },
  "repository": "https://github.com/hmlendea/gfn-electron",
  "keywords": [
    "Electron",
    "gfn",
    "geforcenow",
    "geforce-now"
  ],
  "author": "Hora»õiu Mlendea <io.github.hmlendea.cnzq9@SimpleLogin.fr>",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "anyproxy": "^4.1.3",
    "discord-rich-presence": "^0.0.8",
    "electron-localshortcut": "^3.2.1",
    "express": "^5.1.0",
    "find-process": "^1.4.10",
    "http-proxy": "^1.18.1"
  }
}
